{
  "name": "@nixtus/react-flextable",
  "version": "0.1.11",
  "description": "React based 'table' component that uses FlexBox for responsive design.  Also contains a 'Expand' FlexItem allowing users to put components in an expandable area",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "npm run lint:fix",
    "lint:fix": "eslint ./src --fix",
    "lint:run": "eslint ./src",
    "lint:run:nowarnings": "eslint ./src --quiet",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:all": "eslint ./src/app && jest",
    "test:ci": "npm run lint:run && jest --coverage --silent --colors",
    "test:watch": "npm run test -- --watch --verbose false",
    "build": "webpack --mode production --config webpack/webpack.config.js",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andy9man/react-flextable.git"
  },
  "keywords": [
    "react",
    "flextable",
    "npm",
    "module",
    "flexbox",
    "javascript"
  ],
  "author": "Andrew Ninneman <andy@nixtus.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andy9man/react-flextable/issues"
  },
  "homepage": "http://nixtus.com",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-react-constant-elements": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-style-proptype": "^3.2.2",
    "webpack": "^4.41.2"
  }
}
