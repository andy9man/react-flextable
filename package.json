{
  "name": "@nixtus/react-flextable",
  "version": "1.0.1",
  "description": "React based 'table' component that uses FlexBox for responsive design.  Also contains a 'Expand' FlexItem allowing users to put components in an expandable area",
  "main": "./lib/index.js",
  "scripts": {
    "bump": "bump package.json package-lock.json",
    "clean": "rimraf lib *.tgz",
    "lint": "npm run lint:fix",
    "lint:fix": "eslint ./src --fix",
    "lint:run": "eslint ./src",
    "lint:run:nowarnings": "eslint ./src --quiet",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:all": "eslint ./src && jest",
    "test:ci": "npm run lint:run && jest --coverage --silent --colors",
    "test:watch": "npm run test -- --watch --verbose false",
    "build": "webpack --mode production --config webpack/webpack.config.js",
    "install:fix": "npx npm-force-resolutions",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andy9man/react-flextable.git"
  },
  "keywords": [
    "react",
    "flextable",
    "npm",
    "module",
    "flexbox",
    "javascript"
  ],
  "author": "Andrew Ninneman <andy@nixtus.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andy9man/react-flextable/issues"
  },
  "homepage": "http://nixtus.com",
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-react-constant-elements": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "enzyme": "3.11.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "node-sass": "^5.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-style-proptype": "^3.2.2",
    "styled-components": "^5.2.1"
  },
  "peerDependencies": {
    "react": ">= 16.8.x"
  }
}
