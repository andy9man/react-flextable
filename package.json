{
  "name": "@nixtus/react-flextable",
  "version": "0.1.2",
  "description": "React based 'table' component that uses FlexBox for responsive design.  Also contains a 'Expand' FlexItem allowing users to put components in an expandable area",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "npm run lint:fix",
    "lint:fix": "eslint ./src --fix",
    "lint:run": "eslint ./src",
    "lint:run:nowarnings": "eslint ./src --quiet",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:all": "eslint ./src/app && jest",
    "test:ci": "npm run lint:run && jest --coverage --silent --colors",
    "test:watch": "npm run test -- --watch --verbose false",
    "build": "webpack --mode production --config webpack/webpack.config.js",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andy9man/react-flextable.git"
  },
  "keywords": [
    "react",
    "flextable",
    "npm",
    "module",
    "flexbox",
    "javascript"
  ],
  "author": "Andrew Ninneman <andy@nixtus.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andy9man/react-flextable/issues"
  },
  "homepage": "http://nixtus.com",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.7",
    "babel-cli": "6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.23",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "node-sass": "^4.11.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-style-proptype": "^3.2.2",
    "semantic-release": "^15.13.3",
    "webpack": "^4.29.0"
  }
}